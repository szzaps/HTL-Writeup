Simple code réutilisable ici 0 prise de tête

modulus = [, , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , ]
ciphers = [, , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , , ]

e = 65537

plain_bytes = []
for n, c in zip(modulus, ciphers):
    found = False
    for m in range(256):
        if pow(m, e, n) == c:
            plain_bytes.append(m)
            found = True
            break
    if not found:
        plain_bytes.append(ord('?'))  

flag = bytes(plain_bytes)
flag, flag.decode('utf-8', errors='replace')
